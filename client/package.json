{
  "name": "dp-page",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:dev": "ng serve",
    "start:prod": "ng serve",
    "build-prod": "npm run provideStatic && rm -rf dist && ng build --prod --aot=true --build-optimizer && npm run prepare-index-html && npm run copy-to-assets",
    "prepare-index-html": "mv ./dist/dp-page/index.html ./dist/dp-page/i-want-compression-via-route.html",
    "copy-to-assets": "rm -rf ../../dance/server/docs && cp -r ./dist/dp-page ../../dance/server/docs",
    "provideStatic": "cp -r ./node_modules/country-flag-provider/flags ./src/assets/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.7",
    "@angular/common": "~8.2.7",
    "@angular/compiler": "~8.2.7",
    "@angular/core": "^8.2.14",
    "@angular/forms": "~8.2.7",
    "@angular/platform-browser": "~8.2.7",
    "@angular/platform-browser-dynamic": "~8.2.7",
    "@angular/pwa": "^0.803.23",
    "@angular/router": "~8.2.7",
    "@angular/service-worker": "^8.2.14",
    "array-move": "^2.2.1",
    "bootstrap": "4.3.1",
    "country-flag-provider": "^0.1.1",
    "dance-planner-typeahead": "^1.0.0",
    "distance-calculator-lat-lon": "0.0.1",
    "gzipper": "^3.7.0",
    "moment": "^2.24.0",
    "ng-cards": "0.0.2",
    "ng-range": "0.0.1",
    "ng-responsive-navbar": "^0.1.2",
    "ngx-bootstrap": "^5.2.0",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~15.0.0",
    "@angular/cli": "~8.3.5",
    "@angular/compiler-cli": "~8.2.7",
    "@angular/language-service": "~8.2.7",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
